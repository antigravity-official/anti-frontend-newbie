{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@api/*": ["api/*"],
      "@components/*": ["components/*"],
      "@constants/*": ["constants/*"],
      "@hooks/*": ["hooks/*"],
      "@model/*": ["model/*"],
      "@pages/*": ["pages/*"],
      "@styles": ["styles"],
      "@styles/*": ["styles/*"],
      "@type/*": ["type/*"],
      "@utils/*": ["utils/*"]
    }
  },
  "include": ["src"]
}
